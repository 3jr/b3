<UserControl x:Class="BallOnTiltablePlate.TimoSchmetzer.Algorithm.LissajousJuggler"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controls="clr-namespace:JRapp.WPF;assembly=JRapp.WPF"
             xmlns:controlsB3="clr-namespace:JRapp.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <controlsB3:SettingsSaverB3 x:Name="LissajousJugglerSaver">
        <StackPanel>
            <Expander Header="Harmonic Oszillation Controll" IsExpanded="True">
                <WrapPanel>
                    <StackPanel>
                        <Label Content="xpp = x*k*k"/>
                        <controls:DoubleBox x:Name="Xk" Text="Xk: " />
                        <controls:DoubleBox x:Name="Yk" Text="Yk: " />
                    </StackPanel>
                </WrapPanel>
            </Expander>
        </StackPanel>
    </controlsB3:SettingsSaverB3>
</UserControl>
